#include <iostream>
#include <vector>

int main()
{
    int n;
    std::cin >> n;
    std::cout << n;
    std::vector<int> v;
    v.push_back(n);
    for (int i=0;i<n;i++){
        int temp;
        std::cin>>temp;
        v.push_back(temp);
    }
}
